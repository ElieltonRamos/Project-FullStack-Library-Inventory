{
  "name": "backend-library-inventory",
  "version": "1.0.0",
  "description": "Backend for library inventory management",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "start": "node build/server.js",
    "prestart": "npm run build",
    "dev": "ts-node src/server.ts",
    "predev": "npm run db:reset",
    "build": "tsc --project tsconfig.json",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed",
    "lint": "eslint ./src --ext .ts"
  },
  "keywords": [
    "backend",
    "express"
  ],
  "author": "Elielton Ramos",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "eslint": "^8.57.0",
    "eslint-config-trybe-backend": "^2.1.0",
    "eslint-plugin-sonarjs": "^1.0.3",
    "globals": "^15.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.11.0"
  }
}
